<div id="header" xmlns:jsp="http://java.sun.com/JSP/Page" xmlns:fn="http://java.sun.com/jsp/jstl/functions"
	xmlns:c="http://java.sun.com/jsp/jstl/core" xmlns:spring="http://www.springframework.org/tags" version="2.0">
	<jsp:directive.page contentType="text/html;charset=UTF-8" />
	<jsp:output omit-xml-declaration="yes" />

	<spring:url var="banner"  value="/resources/css/image/banner-graphic.png" />
	<spring:message code="button_home" var="home_label" htmlEscape="true" />
	
	<a href="${contextPath}/" name="${home_label}" title="${home_label}"> <img src="${banner}" /></a>
	
</div>
